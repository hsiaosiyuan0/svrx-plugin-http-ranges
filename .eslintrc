{
    "extends": "@svrx",
    "rules": {
        "quotes": 0,
        "comma-dangle": 0,
        "no-constant-condition": 0,
        "no-cond-assign": 0,
        "arrow-parens": 0,
        "prefer-const": [
            "error",
            {
                "destructuring": "all"
            }
        ],
        "prefer-template": 0,
        "no-shadow": 0,
        "max-classes-per-file": 0,
        "no-plusplus": 0,
        "no-bitwise": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "class-methods-use-this": 0,
        "object-curly-newline": 0,
        "no-await-in-loop": 0
    }
}